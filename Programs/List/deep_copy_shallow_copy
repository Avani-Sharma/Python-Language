# shallow copy: the reference of the outer objects are different and inner objects reference are same.
# that's why when make changes into the original list than also the change would be reflected into the 
# original list 
import copy
li = [[1, 2], 3, 4]
shallow_list = copy.copy(li)
shallow_list[0][0] = 100
print("Original:", li)
print("Shallow:", shallow_list)
print()

# deep copy: the nested objects are also copied and it doesn't change when we make change into the
# copied list 
import copy
li = [[1, 2], 3, 4]
deep = copy.deepcopy(li)
deep[0][0] = 100
print("Original:", li)
print("Deep:", deep)

# implement deep copy and shallow copy on the list
